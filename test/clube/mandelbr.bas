1 X1 = -2.0
2 X2 = 1.0
3 Y1 = -1.2
4 Y2 = Y1+(X2-X1)*192/256
5 XF = (X2-X1)/(256-1)
6 YF = (Y2-Y1)/(192-1)
7 SCREEN 5
10 FOR Y=0 TO 191
20 CY = Y2 - Y*YF
30 FOR X=0 TO 255
40 CX = X1 + X*XF
50 ZX = CX: ZY = CY: C=0
60 XX = ZX*ZX: YY = ZY*ZY
70 IF ((XX + YY) > 4) OR C=255 THEN goto 120
80 ZY = 2*ZX*ZY + CY
90 ZX = XX - YY + CX
100 C=C+1
110 GOTO 60
120 PSET(X, Y),C
130 NEXT X
140 NEXT Y
150 GOTO 150
