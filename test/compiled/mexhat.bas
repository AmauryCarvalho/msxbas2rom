10 SCREEN 5 : CMD TURBO 1
20 FOR B=0 TO 127
30 X4 = B * B : M = -128 : A = SQR(16384 - X4)
40 FOR I = -A TO A STEP 3
50 R = SQR(X4+I*I)/128 : F = COS(16*R)*(1-R)*2 : Y = I/5 + F*32
60 IF Y<=M THEN GOTO 90 ELSE M=Y
70 Y = 128+Y : X = 128 + B
80 PSET (X, 236-Y) : X = 128-B : PSET(X,236-Y)
90 NEXT I
100 NEXT B
110 GOTO 110

