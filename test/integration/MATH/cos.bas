05 CMD TURBO 1
10 DIM M(255):COLOR ,0,0:SCREEN 8
20 PI=4*ATN(1)
30 A=COS(PI/4)
40 FOR Y=141 TO 1 STEP -1
50 E=A*Y:C=Y-70:C=C*C
60 FOR X=1 TO 141
70 D=X-70:Z=80*EXP(-.001*(C+D*D)):X1=X+E:Y1=Z+E
75 CY=Y/141*255
80 'IF Y1>=M(X1) THEN M(X1)=Y1
85 IF X>1 THEN LINE-(X1+8,191-Y1),CY ELSE PSET(X1+8,191-Y1),CY
90 NEXT:NEXT
100 I$=INPUT$(1)

