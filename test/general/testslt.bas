10 CLS
11 I = INP(&HA8)
12 PRINT "PAGE P0 P1 P2 P3"
13 PRINT "SLOT "; 
14 FOR P = 0 TO 3 : PRINT STR$(I MOD 4); " "; : I = I / 4 : NEXT P
15 PRINT " " : PRINT " " : PRINT "SLOT S0 S1 S2 S3" : PRINT "EXPA ";
16 FOR I = &HFCC1 TO &HFCC4:PRINT RIGHT$(HEX$(PEEK(I)+&H100),2);" ";:NEXT I 
17 PRINT " " : PRINT " "
19 PRINT "SLOTS EXPANSION TABLE"
20 PRINT "S/P  0   1   2   3"
30 FOR S = 0 TO 3
40 I = PEEK(&HFCC5+S)
50 PRINT S; " ";
60 FOR K = 0 TO 3
70 PRINT (I MOD 4); " ";
80 I = I / 4
90 NEXT K 
100 PRINT " "
110 NEXT S
120 INPUT "PRESS ENTER TO CONTINUE"; I
130 CLS 
140 PRINT "SLOTS ATTRIBUTES" 
150 PRINT "SL SS P0 P1 P2 P3"
160 P = 0 : S = 0 : SS = 0
170 FOR I = &HFCC9 TO &HFCC9+63
180 F = PEEK(I) 
190 IF P = 0 THEN PRINT STR$(S); " "; STR$(SS); " ";
200 IF F = 0 THEN PRINT "   "; ELSE PRINT RIGHT$(HEX$(F+&H100), 2); " ";
210 P = P + 1 
220 IF P > 3 THEN P = 0 : PRINT " " : SS = SS + 1 : IF SS > 3 THEN SS = 0 : S = S + 1
230 NEXT I

